    return

inc: ; Subrutina para inrementar en modo configuraci?n
    btfss bandera_config, 0
    return

    btfsc config_state, 0
    goto config_2inc
    goto config_1inc

    config_1inc:
 btfsc estados, 0
 goto inc_dias
 btfsc estados, 1
 goto inc_ma
