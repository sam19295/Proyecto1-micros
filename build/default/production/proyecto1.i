    movwf alarma_bandera
    return

configclk:
    banksel OSCCON ; cambiamos a banco de OSCCON
    bsf OSCCON, 0 ; ((OSCCON) and 07Fh), 0 -> 1, Usamos reloj interno
    bsf OSCCON, 6
    bsf OSCCON, 5
    bcf OSCCON, 4 ; IRCF<2:0> -> 110 4MHz
    return

config_tmr0:
    banksel OPTION_REG ; Cambiamos a banco de OPTION_REG
    bcf OPTION_REG, 5 ; ((OPTION_REG) and 07Fh), 5 = 0 --> TIMER0 como temporizador
    bcf OPTION_REG, 3 ; Prescaler a TIMER0
    bcf OPTION_REG, 2 ; ((OPTION_REG) and 07Fh), 2
    bcf OPTION_REG, 1 ; ((OPTION_REG) and 07Fh), 1
    bcf OPTION_REG, 0 ; ((OPTION_REG) and 07Fh), 0 Prescaler de 1 : 2
    banksel TMR0 ; Cambiamos a banco 0 de TIMER0
    movlw 6 ; Cargamos el valor 6 a W
    movwf TMR0 ; Cargamos el valor de W a TIMER0 para 2mS de delay
    bcf ((INTCON) and 07Fh), 2 ; Borramos la bandera de interrupcion
    return

config_tmr1:
    banksel T1CON ; Cambiamos a banco de tmr1
    bcf ((T1CON) and 07Fh), 1 ; Reloj interno
    bcf ((T1CON) and 07Fh), 3 ; Apagamos LP
    bsf ((T1CON) and 07Fh), 5 ; Prescaler 1:8
    bsf ((T1CON) and 07Fh), 4
    bcf ((T1CON) and 07Fh), 6 ; tmr1 siempre contando
    bsf ((T1CON) and 07Fh), 0 ; Encender tmr1
    call reset_tmr1
    return

config_tmr2:
    banksel PR2
    movlw 243 ; Para delay de 62.5 mS
    movwf PR2
    banksel T2CON
    bsf ((T2CON) and 07Fh), 1 ; Prescaler de 1:16
    bsf ((T2CON) and 07Fh), 0
    bsf ((T2CON) and 07Fh), 6 ; Postscaler de 1:16
    bsf ((T2CON) and 07Fh), 5
    bsf ((T2CON) and 07Fh), 4
    bsf ((T2CON) and 07Fh), 3
    bsf ((T2CON) and 07Fh), 2 ; tmr2 encendido
    return

configint:
    banksel IOCB
    bsf IOCB, 0 ; Interrupcion en ((PORTB) and 07Fh), 0
    bsf IOCB, 1 ; Interrupcion en ((PORTB) and 07Fh), 1
    bsf IOCB, 2 ; Interrupcion en ((PORTB) and 07Fh), 2
    bsf IOCB, 3 ; Interrupcion en ((PORTB) and 07Fh), 3
    bsf IOCB, 4 ; Interrupcion en ((PORTB) and 07Fh), 4
    banksel PIE1
    bsf ((PIE1) and 07Fh), 0 ; Habilitamos interrupcion TMR1
    bsf ((PIE1) and 07Fh), 1 ; Habilitamos interrupcion TMR2
    banksel INTCON
    bsf ((INTCON) and 07Fh), 6
    bsf ((INTCON) and 07Fh), 7 ; Habilitamos interrupciones
    bsf ((INTCON) and 07Fh), 5 ; Habilitamos interrupcion TMR0
    bcf ((INTCON) and 07Fh), 2 ; Limpiamos bandera de TMR0
    bcf ((PIR1) and 07Fh), 0 ; Limpiamos bandera de TMR1
    bcf ((PIR1) and 07Fh), 1 ; Limpiamos bandera de TMR2
    bsf ((INTCON) and 07Fh), 3 ; Habilitamos interrupcion PORTB
    bcf ((INTCON) and 07Fh), 0 ; Limpiamos bandera de PORTB
    return

reset_tmr0:
    banksel TMR0 ; cambiamos de banco
    movlw 6
    movwf TMR0 ; delay 4.44mS
    bcf ((INTCON) and 07Fh), 2
    return

reset_tmr1:
    banksel TMR1H
    movlw 0x0B ; Configuraci?n tmr1 H
    movwf TMR1H
    movlw 0xDC ; Configuraci?n tmr1 L
    movwf TMR1L ; tmr1 a 500 mS
    bcf ((PIR1) and 07Fh), 0 ; Limpiar bandera de tmr1

tm0:
    call reset_tmr0
    call valores
    return

tm1:
    call reset_tmr1
    incf cont1
    comf medio
    return

tm2:
    bcf ((PIR1) and 07Fh), 1 ; Limpiar la bandera de tmr2
    return

intb: ; Interrupciones de puerto B
    btfss PORTB, 0
    call cambiarestado ; Ver si fue B1
    btfss PORTB, 1
    call configuracion ; Ver si fue B2
    btfss PORTB, 2
    call inc ; Ver si fue B3
    btfss PORTB, 3
    call decr ; Ver si fue B4
    btfss PORTB, 4
    call cambiar ; Ver si fue B5
    bcf ((INTCON) and 07Fh), 0 ; Limpiar bandera de puerto B
    return

cambiarestado:
    btfsc estados, 0 ; Verificar en qu? estado se encuentra la FSM
    goto S2_cambio ; para verificar cu?l deber?a de ser el siguiente
    btfsc estados, 1
    goto S3_cambio
    btfsc estados, 2
    goto S0_cambio
    goto S1_cambio

    S0_cambio:
 bcf estados, 0
 bcf estados, 1
 bcf estados, 2
 return

    S1_cambio:
 bsf estados, 0
 bcf estados, 1
 bcf estados, 2
 return

    S2_cambio:
 bcf estados, 0
 bsf estados, 1
 bcf estados, 2
 return

    S3_cambio:
 bcf estados, 0
 bcf estados, 1
 bsf estados, 2
 return

configuracion:
    comf bandera_config ; Habilitar o deshabilitar bandera de configuraciones
    return

set_displayS0: ; Mostrar en el display horas y minutos de reloj
    movf unidades_min, w
