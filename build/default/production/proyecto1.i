    movwf display+1

    movf decenas_min, w
    call tabla
    movwf display+2

    movf unidades_hrs, w
    call tabla
    movwf display+3

    movf decenas_hrs, w
    call tabla
    movwf display
    return

set_displayS1: ; Mostrar en el display dia y mes de fecha
    movf unidades_mes, w
    call tabla
    movwf display+1

    movf decenas_mes, w
    call tabla
    movwf display+2

    movf unidades_dia, w
    call tabla
    movwf display+3

    movf decenas_dia, w
