push:
    movwf tempw ; Se guarda W en el registro temporal
    swapf STATUS, W
    movwf temp_status ; Se guarda STATUS en el registro temporal

isr:
    banksel INTCON
    btfsc ((INTCON) and 07Fh), 2 ; Ver si bandera de TMR0 se encendi?
    call tm0
    btfsc ((PIR1) and 07Fh), 0 ; Ver si bandera de TMR1 se encendi?
    call tm1
    btfsc ((PIR1) and 07Fh), 1 ; Ver si bandera de TMR2 se encendi?
    call tm2
    btfsc ((INTCON) and 07Fh), 0
    call intb

pop:
    swapf temp_status, W
    movwf STATUS ; Se recupera el valor de STATUS
    swapf tempw, F
    swapf tempw, W ; Se recupera el valor de W
    retfie

